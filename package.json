{
  "name": "zero-feeds",
  "version": "1.6.0",
  "description": "Read your RSS feeds within your Cozy.",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/pierrerousseau/zero-feeds.git"
  },
  "main": "server.coffee",
  "dependencies": {
    "americano": "0.3.11",
    "americano-cozy": "0.2.11",
    "cozy-i18n-helper": "0.0.4",
    "iconv": "2.1.11"
  },
  "devDependencies": {
    "should": "= 1.2.0",
    "mocha": "= 1.6.0",
    "chai": "= 1.3.0"
  },
  "scripts": {
    "test": "cake tests",
    "start": "node server.js"
  },
  "cozy-displayName": "zero-feeds",
  "cozy-permissions": {
    "feed": {
      "description": "Allow you to manage your feeds"
    },
    "param": {
      "description": "to clean"
    },
    "zfparam": {
      "description": "Allow you to change the options of the zero feed app"
    },
    "CozyInstance": {
      "description": "Read language setting"
    }
  }
}
